#include <iostream>
#include <vector>
using namespace std;

vector<bool> eratos(int m) {
    vector<bool> prime(m + 1, true);
    prime[0] = prime[1] = false;
    for (int p = 2; p * p <= m; ++p) {
        if (prime[p] == true) {
            for (int i = p * p; i < m; i+=p) {
                prime[i] = false;
            }
        }
    }
    return prime;
}

int main() {
    int n;
    cin >> n;
    vector<int> num(n);
    for (int i = 0; i < n; ++i) {
        cin >> num[i];
    }
    
    vector<bool> prime = eratos(1000000);
    int count {0};
    for (int i = 0; i < n; ++i) {
        if (prime[num[i]]) {
            ++count;
        }
    }
    cout << count;
    
}
